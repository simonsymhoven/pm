<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="476.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.aktien.modal.ModalController">
   <children>
      <Button fx:id="addAktie" layoutX="511.0" layoutY="408.0" mnemonicParsing="false" onAction="#addAktie" prefHeight="37.0" prefWidth="98.0" stylesheets="@../fullpackstyling.css" text="Hinzufügen" />
      <Label layoutX="40.0" layoutY="42.0" text="Aktie hinzufügen" textFill="#343f4a">
         <font>
            <Font size="19.0" />
         </font>
      </Label>

      <Circle fill="#f94344" layoutX="609.0" layoutY="28.0" radius="14.0" stroke="WHITE" strokeType="INSIDE" />
      <Label fx:id="lblClose" alignment="CENTER" layoutX="595.0" layoutY="20.0" prefHeight="17.0" prefWidth="28.0" text="X" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="symbol" layoutX="40.0" layoutY="132.0" promptText="Symbol" stylesheets="@../fullpackstyling.css" />
      <Button fx:id="search" layoutX="221.0" layoutY="122.0" mnemonicParsing="false" onAction="#searchAktie" prefHeight="37.0" prefWidth="98.0" stylesheets="@../fullpackstyling.css" text="Suchen" />

   </children>
</AnchorPane>
